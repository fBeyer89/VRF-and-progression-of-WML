@article{hardyAmyloidHypothesisAlzheimer2002,
  title = {The {{Amyloid Hypothesis}} of {{Alzheimer}}\&\#039;s {{Disease}}: {{Progress}} and {{Problems}} on the {{Road}} to {{Therapeutics}}},
