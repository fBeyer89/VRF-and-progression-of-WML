@article{Kenna_2013,
  title = {Fasting Plasma Insulin and the Default Mode Network in Women at Risk for {{Alzheimer}}'s Disease},
  author = {Kenna, H. and Hoeft, F. and Kelley, R. and Wroolie, T. and DeMuth, B. and Reiss, A. and Rasgon, N.},
  year = {2013},
  month = mar,
  journal = {Neurobiol Aging},
  volume = {34},
  number = {3},
  pages = {641--649},
  issn = {0197-4580},
  doi = {10.1016/j.neurobiolaging.2012.06.006},
  abstract = {Brain imaging studies in Alzheimer's disease research have demonstrated structural and functional perturbations in the hippocampus and default mode network (DMN). Additional evidence suggests risk for pathological brain aging in association with insulin resistance (IR). This study piloted investigation of associations of IR with DMN-hippocampal functional connectivity among postmenopausal women at risk for Alzheimer's disease. Twenty middle-aged women underwent resting state functional magnetic resonance imaging. Subjects were dichotomized relative to fasting plasma insulin levels (i.e., {$>$} 8 mu IU/mL [n = 10] and {$>$} 8 mu IU/mL [n = 10]), and functional connectivity analysis contrasted their respective blood oxygen level-dependent signal correlation between DMN and hippocampal regions. Higher-insulin women had significantly reduced positive associations between the medial prefrontal cortex and bilateral parahippocampal regions extending to the right hippocampus, and conversely, between the left and right hippocampus and medial prefrontal cortex. Neuropsychological data (all within normal ranges) also showed significant differences with respect to executive functioning and global intelligence. The results provide further evidence of deleterious effects of IR on the hippocampus and cognition. Further imaging studies of the IR-related perturbations in DMN-hippocampal functional connectivity are needed. (c) 2013 Elsevier Inc. All rights reserved.},
  langid = {english},
  keywords = {allele,brain,connectivity,default mode network,functional connectivity,functional mri,glucose,hippocampus,individuals,insulin resistance,memory performance,postmenopausal women,prevalence,resistance,risk for alzheimer's disease}
}

