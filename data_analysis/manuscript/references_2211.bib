  volume = {161},
  number = {1},
  pages = {119--132},
  issn = {0092-8674},
  doi = {10.1016/j.cell.2015.03.008},
  abstract = {The global rise in the prevalence of obesity and associated co-morbidities such as type 2 diabetes, cardiovascular disease, and cancer represents a major public health concern. The biological response to increased consumption of palatable foods or a reduction in energy expenditure is highly variable between individuals. A more detailed mechanistic understanding of the molecular, physiological, and behavioral pathways involved in the development of obesity in susceptible individuals is critical for identifying effective mechanism-based preventative and therapeutic interventions.}
}

