@article{ekenze23,
  title = {Inflammatory Biomarkers and {{MRI}} Visible Perivascular Spaces: {{The Framingham Heart Study}}},
  shorttitle = {Inflammatory Biomarkers and {{MRI}} Visible Perivascular Spaces},
  author = {Ekenze, Oluchi and Pinheiro, Adlin and Demissie, Serkalem and Charidimou, Andreas and Beiser, Alexa S. and Vasan, Ramachandran S. and Benjamin, Emelia J. and DeCarli, Charles and Seshadri, Sudha and Romero, Jose R.},
