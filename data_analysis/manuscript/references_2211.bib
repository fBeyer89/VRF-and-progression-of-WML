@article{moranTypeDiabetesMellitus2015,
  title = {Type 2 Diabetes Mellitus and Biomarkers of Neurodegeneration},
  author = {Moran, Chris and Beare, Richard and Phan, Thanh G. and Bruce, David G. and Callisaya, Michele L. and Srikanth, Velandai},
