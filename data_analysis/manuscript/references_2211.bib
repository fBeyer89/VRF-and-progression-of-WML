@article{fairCorrectionRespiratoryArtifacts2018,
  title = {Correction of Respiratory Artifacts in {{MRI}} Head Motion Estimates},
  author = {Fair, Damien A. and {Miranda-Dominguez}, Oscar and Snyder, Abraham Z. and Perrone, Anders and Earl, Eric A. and Van, Andrew N. and Koller, Jonathan M. and Feczko, Eric and Klein, Rachel L. and Mirro, Amy E. and Hampton, Jacqueline M. and Adeyemo, Babatunde and Laumann, Timothy O. and Gratton, Caterina and Greene, Deanna J. and Schlaggar, Bradley L. and Hagler, Don and Watts, Richard and Garavan, Hugh and Barch, Deanna M. and Nigg, Joel T. and Petersen, Steven E. and Dale, Anders and {Feldstein-Ewing}, Sarah W. and Nagel, Bonnie J. and Dosenbach, Nico U. F.},
  year = {2018},
  journal = {bioRxiv},
  pages = {337360},
  doi = {10.1101/337360},
  abstract = {Head motion represents one of the greatest technical obstacles for brain MRI. Accurate detection of artifacts induced by head motion requires precise estimation of movement. However, this estimation may be corrupted by factitious effects owing to main field fluctuations generated by body motion. In the current report, we examine head motion estimation in multiband resting state functional connectivity MRI (rs-fcMRI) data from the Adolescent Brain and Cognitive Development (ABCD) Study and a comparison `single-shot' dataset from Oregon Health \&amp;amp; Science University. We show unequivocally that respirations contaminate movement estimates in functional MRI and that respiration generates apparent head motion not associated with degraded quality of functional MRI. We have developed a novel approach using a band-stop filter that accurately removes these respiratory effects. Subsequently, we demonstrate that utilizing this filter improves post-processing data quality. Lastly, we demonstrate the real-time implementation of motion estimate filtering in our FIRMM (Framewise Integrated Real-Time MRI Monitoring) software package.}
}

@article{fairCorrectionRespiratoryArtifacts2020,
  title = {Correction of Respiratory Artifacts in {{MRI}} Head Motion Estimates},
  author = {Fair, Damien A. and {Miranda-Dominguez}, Oscar and Snyder, Abraham Z. and Perrone, Anders and Earl, Eric A. and Van, Andrew N. and Koller, Jonathan M. and Feczko, Eric and Tisdall, M. Dylan and {van der Kouwe}, Andre and Klein, Rachel L. and Mirro, Amy E. and Hampton, Jacqueline M. and Adeyemo, Babatunde and Laumann, Timothy O. and Gratton, Caterina and Greene, Deanna J. and Schlaggar, Bradley L. and Hagler, Donald J. and Watts, Richard and Garavan, Hugh and Barch, Deanna M. and Nigg, Joel T. and Petersen, Steven E. and Dale, Anders M. and {Feldstein-Ewing}, Sarah W. and Nagel, Bonnie J. and Dosenbach, Nico U. F.},
  year = {2020/03/01/},
  journal = {NeuroImage},
  volume = {208},
  pages = {116400},
  issn = {1053-8119},
  doi = {10.1016/j.neuroimage.2019.116400},
  abstract = {Head motion represents one of the greatest technical obstacles in magnetic resonance imaging (MRI) of the human brain. Accurate detection of artifacts induced by head motion requires precise estimation of movement. However, head motion estimates may be corrupted by artifacts due to magnetic main field fluctuations generated by body motion. In the current report, we examine head motion estimation in multiband resting state functional connectivity MRI (rs-fcMRI) data from the Adolescent Brain and Cognitive Development (ABCD) Study and comparison `single-shot' datasets. We show that respirations contaminate movement estimates in functional MRI and that respiration generates apparent head motion not associated with functional MRI quality reductions. We have developed a novel approach using a band-stop filter that accurately removes these respiratory effects from motion estimates. Subsequently, we demonstrate that utilizing a band-stop filter improves post-processing fMRI data quality. Lastly, we demonstrate the real-time implementation of motion estimate filtering in our FIRMM (Framewise Integrated Real-Time MRI Monitoring) software package.}
}

