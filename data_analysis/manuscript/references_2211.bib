  title = {Intentional Weight Loss in Overweight and Obese Individuals and Cognitive Function: A Systematic Review and Meta-Analysis},
  author = {Siervo, M. and Arnold, R. and Wells, J. C. K. and Tagliabue, A. and Colantuoni, A. and Albanese, E. and Brayne, C. and Stephan, B. C. M.},
