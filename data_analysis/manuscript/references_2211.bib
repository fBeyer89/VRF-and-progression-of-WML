  author = {Finsterwalder, S. and Vlegels, N. and Gesierich, B. and Araque Caballero, {\relax M{\'A}} and Weaver, N. A. and Franzmeier, N. and Georgakis, M. K. and Konieczny, M. J. and Koek, H. L. and Karch, C. M. and {Graff-Radford}, N. R. and Salloway, S. and Oh, H. and Allegri, R. F. and Chhatwal, J. P. and Jessen, F. and D{\"u}zel, E. and Dobisch, L. and Metzger, C. and Peters, O. and Incesoy, E. I. and Priller, J. and Spruth, E. J. and Schneider, A. and Flie{\ss}bach, K. and Buerger, K. and Janowitz, D. and Teipel, S. J. and Kilimann, I. and Laske, C. and Buchmann, M. and Heneka, M. T. and Brosseron, F. and Spottke, A. and Roy, N. and {Ertl-Wagner}, B. and Scheffler, K. and Seo, S. W. and Kim, Y. and Na, D. L. and Kim, H. J. and Jang, H. and Ewers, M. and Levin, J. and Schmidt, R. and Pasternak, O. and Dichgans, M. and Biessels, G. J. and Duering, M.},
