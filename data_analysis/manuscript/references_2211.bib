@article{vuorinenChangesVascularRisk2011,
  title = {Changes in {{Vascular Risk Factors}} from {{Midlife}} to {{Late Life}} and {{White Matter Lesions}}: {{A}} 20-{{Year Follow-Up Study}}},
  author = {Vuorinen, M. and Solomon, A. and Rovio, S. and Nieminen, L. and K{\aa}reholt, I. and Tuomilehto, J. and Soininen, H. and Kivipelto, M.},
  year = {2011},
  journal = {Dementia and Geriatric Cognitive Disorders},
  volume = {31},
  number = {2},
  pages = {119--125},
  issn = {1420-8008},
  doi = {10.1159/000323810},
  abstract = {\emph{Background/Aims:} This study investigated the relation of midlife blood pressure, total cholesterol, body mass index (BMI), their changes over time, apolipoprotein E, and white matter lesions (WML). \emph{Methods:} Participants of the Cardiovascular Risk Factors, Aging and Incidence of Dementia study were derived from random, population-based samples previously surveyed in 1972, 1977, 1982 or 1987. In 1998, 1,449 (73\%) individuals aged 65{\textendash}79 years were re-examined (average follow-up 21 years). A subpopulation (n = 112) was scanned with a 1.5-tesla MRI scanner in 1998, and WML were assessed from fluid-attenuated inversion recovery images using a semi-quantitative visual rating scale. \emph{Results:} Risk of late-life WML was related to midlife overweight (relative risk = 2.53; 95\% CI = 1.70{\textendash}2.89), obesity (2.94; 2.44{\textendash}3.03), and hypertension (2.73; 1.81{\textendash}3.08), even after adjustments for several confounding factors. Elevated BMI ({$>$}25) (2.26; 1.42{\textendash}2.62) and hypertension (3.14; 1.83{\textendash}3.40) from midlife to late life also increased the risk of WML. In addition, an association with WML was seen for decreasing blood pressure (hypertension at midlife but not at late life) (3.25; 2.46{\textendash}3.41), even after controlling for antihypertensive treatment. Lipid-lowering drugs had a protective effect against WML (0.13; 0.02{\textendash}0.59). \emph{Conclusions:} These results indicate that early and sustained vascular risk factor control is associated with a lower likelihood of having more severe WML in late life.}
}

