  langid = {english},
  keywords = {Alzheimer's disease,amyloid positron emission tomography,cognition,corpus callosum,executive functions,fluid-attenuated inversion recovery,magnetic resonance imaging,memory,splenium,white matter hyperintensities},
  file = {/data/hu_fbeyer/Zotero/storage/GX544EVI/Garnier-Crussard et al. - 2022 - White matter hyperintensity topography in Alzheime.pdf;/data/hu_fbeyer/Zotero/storage/HW6A9QZR/alz.html}
}

@article{garnier-crussard23,
  title = {White Matter Hyperintensities in {{Alzheimer}}'s Disease: {{Beyond}} Vascular Contribution},
  shorttitle = {White Matter Hyperintensities in {{Alzheimer}}'s Disease},
  author = {{Garnier-Crussard}, Antoine and Cotton, Fran{\c c}ois and {Krolak-Salmon}, Pierre and Ch{\'e}telat, Ga{\"e}l},
