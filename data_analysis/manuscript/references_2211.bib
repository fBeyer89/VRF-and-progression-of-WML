@article{abdullahNumberYearsLived2011,
  title = {The Number of Years Lived with Obesity and the Risk of All-Cause and Cause-Specific Mortality},
  author = {Abdullah, Asnawi and Wolfe, Rory and Stoelwinder, Johannes U. and {de Courten}, Maximilian and Stevenson, Christopher and Walls, Helen L. and Peeters, Anna},
  year = {2011},
  journal = {International Journal of Epidemiology},
  volume = {40},
  number = {4},
  pages = {985--996},
  issn = {0300-5771},
  doi = {10.1093/ije/dyr018},
  abstract = {Background The role of the duration of obesity as an independent risk factor for mortality has not been investigated. The aim of this study was to analyse the association between the duration of obesity and the risk of mortality.Methods A total of 5036 participants (aged 28{\textendash}62 years) of the Framingham Cohort Study were followed up every 2 years from 1948 for up to 48 years. The association between obesity duration and all-cause and cause-specific mortality was analysed using time-dependent Cox models adjusted for body mass index. The role of biological intermediates and chronic diseases was also explored.Results The adjusted hazard ratio (HR) for mortality increased as the number of years lived with obesity increased. For those who were obese for 1{\textendash}4.9, 5{\textendash}14.9, 15{\textendash}24.9 and {$\geq$}25 years of the study follow-up period, adjusted HRs for all-cause mortality were 1.51 [95\% confidence interval (CI) 1.27{\textendash}1.79], 1.94 (95\% CI 1.71{\textendash}2.20), 2.25 (95\% CI 1.89{\textendash}2.67) and 2.52 (95\% CI 2.08{\textendash}3.06), respectively, compared with those who were never obese. A dose{\textendash}response relation between years of duration of obesity was also clear for all-cause, cardiovascular, cancer and other-cause mortality. For every additional 2 years of obesity, the HRs for all-cause, cardiovascular disease, cancer and other-cause mortality were 1.06 (95\% CI 1.05{\textendash}1.07), 1.07 (95\% CI 1.05{\textendash}1.08), 1.03 (95\% CI 1.01{\textendash}1.05) and 1.07 (95\% CI 1.05{\textendash}1.11), respectively.Conclusions The number of years lived with obesity is directly associated with the risk of mortality. This needs to be taken into account when estimating its burden on mortality.}
}

