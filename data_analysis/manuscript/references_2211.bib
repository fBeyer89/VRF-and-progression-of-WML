@article{palhaugenBrainAmyloidVascular2021a,
  title = {Brain Amyloid and Vascular Risk Are Related to Distinct White Matter Hyperintensity Patterns},
  author = {P{\aa}lhaugen, Lene and Sudre, Carole H. and Tecelao, Sandra and Nakling, Arne and Almdahl, Ina S. and Kalheim, Lisa F. and Cardoso, M. Jorge and Johnsen, Stein H. and Rongve, Arvid and Aarsland, Dag and Bj{\o}rnerud, Atle and Selnes, Per and Fladby, Tormod},
  year = {2021},
  month = may,
  journal = {Journal of Cerebral Blood Flow \& Metabolism},
  volume = {41},
  number = {5},
  pages = {1162--1174},
  issn = {0271-678X},
  doi = {10.1177/0271678X20957604},
  abstract = {White matter hyperintensities (WMHs) are associated with vascular risk and Alzheimer?s disease. In this study, we examined relations between WMH load and distribution, amyloid pathology and vascular risk in 339 controls and cases with either subjective (SCD) or mild cognitive impairment (MCI). Regional deep (DWMH) and periventricular (PWMH) WMH loads were determined using an automated algorithm. We stratified on A?1-42 pathology (A?+/?) and analyzed group differences, as well as associations with Framingham Risk Score for cardiovascular disease (FRS-CVD) and age. Occipital PWMH (p?=?0.001) and occipital DWMH (p?=?0.003) loads were increased in SCD-A?+ compared with A?? controls. In MCI-A?+ compared with A?? controls, there were differences in global WMH (p?=?0.003), as well as occipital DWMH (p?=?0.001) and temporal DWMH (p?=?0.002) loads. FRS-CVD was associated with frontal PWMHs (p?=?0.003) and frontal DWMHs (p?=?0.005), after adjusting for age. There were associations between global and all regional WMH loads and age. In summary, posterior WMH loads were increased in SCD-A?+ and MCI-A?+ cases, whereas frontal WMHs were associated with vascular risk. The differences in WMH topography support the use of regional WMH load as an early-stage marker of etiology.}
}

