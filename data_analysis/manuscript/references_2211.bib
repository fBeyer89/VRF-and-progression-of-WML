@article{Lemieux_1993,
  title = {Sex Differences in the Relation of Visceral Adipose Tissue Accumulation to Total Body Fatness},
  author = {Lemieux, S. and Prud'homme, D. and Bouchard, C. and Tremblay, A. and Despres, J. P.},
