@article{opelPrefrontalGrayMatter2017,
  title = {Prefrontal Gray Matter Volume Mediates Genetic Risks for Obesity},
  author = {Opel, N. and Redlich, R. and Kaehler, C. and Grotegerd, D. and Dohm, K. and Heindel, W. and Kugel, H. and Thalamuthu, A. and Koutsouleris, N. and Arolt, V. and Teuber, A. and Wersching, H. and Baune, B. T. and Berger, K. and Dannlowski, U.},
  year = {2017},
