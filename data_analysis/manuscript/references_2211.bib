@article{singh-manouxObesityPhenotypesMidlife2012,
  title = {Obesity Phenotypes in Midlife and Cognition in Early Old Age},
  author = {{Singh-Manoux}, Archana and Czernichow, S{\'e}bastien and Elbaz, Alexis and Dugravot, Aline and Sabia, S{\'e}verine and {Hagger-Johnson}, Gareth and Kaffashian, Sara and Zins, Marie and Brunner, Eric J. and Nabi, Hermann and Kivim{\"a}ki, Mika},
  year = {2012},
  journal = {Neurology},
  volume = {79},
  number = {8},
  pages = {755},
  abstract = {Objective: To examine the association of body mass index (BMI) and metabolic status with cognitive function and decline. Methods: A total of 6,401 adults (71.2\% men), aged 39{\textendash}63 years in 1991{\textendash}1993, provided data on BMI (normal weight 18.5{\textendash}24.9 kg/m2, overweight 25{\textendash}29.9 kg/m2; and obese {$\geq$}30 kg/m2) and metabolic status (abnormality defined as 2 or more of 1) triglycerides {$\geq$}1.69 mmol/L or lipid-lowering drugs, 2) systolic blood pressure {$\geq$}130 mm Hg, diastolic blood pressure {$\geq$}85 mm Hg, or antihypertensive drugs, 3) glucose {$\geq$}5.6 mmol/L or medications for diabetes, and 4) high-density lipoprotein cholesterol \&amp;lt;1.04 mmol/L for men and \&amp;lt;1.29 mmol/L for women). Four cognitive tests (memory, reasoning, semantic, and phonemic fluency) were administered in 1997{\textendash}1999, 2002{\textendash}2004, and 2007{\textendash}2009, standardized to z scores, and averaged to yield a global score. Results: Of the participants, 31.0\% had metabolic abnormalities, 52.7\% were normal weight, 38.2\% were overweight, and 9.1\% were obese. Among the obese, the global cognitive score at baseline (p = 0.82) and decline (p = 0.19) over 10 years was similar in the metabolically normal and abnormal groups. In the metabolically normal group, the 10-year decline in the global cognitive score was similar (p for trend = 0.36) in the normal weight (-0.40; 95\% confidence interval [CI] -0.42 to -0.38), overweight (-0.42; 95\% CI -0.45 to -0.39), and obese (-0.42; 95\% CI -0.50 to -0.34) groups. However, in the metabolically abnormal group, the decline on the global score was faster among obese (-0.49; 95\% CI -0.55 to -0.42) than among normal weight individuals (-0.42; 95\% CI -0.50 to -0.34), (p = 0.03). Conclusions: In these analyses the fastest cognitive decline was observed in those with both obesity and metabolic abnormality. BMI=body mass index; HDL=high-density lipoprotein; MHO=metabolically healthy obesity}
}

