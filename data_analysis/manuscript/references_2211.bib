@article{power15,
  title = {Smoking and White Matter Hyperintensity Progression: {{The ARIC-MRI Study}}},
  shorttitle = {Smoking and White Matter Hyperintensity Progression},
  author = {Power, Melinda C. and Deal, Jennifer A. and Sharrett, A. Richey and Jack, Clifford R. and Knopman, David and Mosley, Thomas H. and Gottesman, Rebecca F.},
  year = {2015},
  month = feb,
  journal = {Neurology},
  volume = {84},
  number = {8},
  pages = {841--848},
  publisher = {{Wolters Kluwer Health, Inc. on behalf of the American Academy of Neurology}},
  issn = {0028-3878, 1526-632X},
  doi = {10.1212/WNL.0000000000001283},
  urldate = {2023-08-25},
  abstract = {Objective: Our objective was to examine the link between smoking and smoking history, including smoking intensity and cessation, overall and by race, in a biracial prospective cohort study. Methods: A subset of Atherosclerosis Risk in Communities Study participants (n = 972, 49\% black) completed brain MRI scans twice (1993{\textendash}1995 and 2004{\textendash}2006). We defined white matter hyperintensity (WMH) progression as an increase of {$\geq$}2 points on the 9-point Cardiovascular Health Study scale across scans. Participants reported information on smoking behavior at the baseline MRI and at 2 prior study visits, approximately 3 and 6 years before baseline. We used adjusted logistic regression to evaluate the association between smoking variables and WMH progression in the total sample and separately by race (black and white). Results: We found WMH progression in 23\% of participants (30\% of black participants, 17\% of white participants). Overall, being a current smoker 6 years before baseline was associated with WMH progression. In race-stratified analyses, we found adverse associations with smoking status at multiple time points and persistent smoking in white but not in black participants. However, we found no statistical support for effect modification by race for most of these analyses. Increasing pack-years of smoking was associated with greater risk of WMH progression, while time since quitting and age at smoking initiation were not associated with WMH progression, with little indication of differences in these associations by race. Conclusions: Our findings concur with previous studies suggesting a relationship between smoking and WMH progression, and further demonstrate a dose-dependent association.},
  chapter = {Article},
  copyright = {{\copyright} 2015 American Academy of Neurology},
  langid = {english},
  pmid = {25632094},
  file = {/data/hu_fbeyer/Zotero/storage/7I3YGFK3/Power et al. - 2015 - Smoking and white matter hyperintensity progressio.pdf}
}

