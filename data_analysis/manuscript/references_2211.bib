@article{schilling13,
  title = {{{APOE}} Genotype and {{MRI}} Markers of Cerebrovascular Disease: {{Systematic}} Review and Meta-Analysis},
  shorttitle = {{{APOE}} Genotype and {{MRI}} Markers of Cerebrovascular Disease},
  author = {Schilling, Sabrina and DeStefano, Anita L. and Sachdev, Perminder S. and Choi, Seung Hoan and Mather, Karen A. and DeCarli, Charles D. and Wen, Wei and H{\o}gh, Peter and Raz, Naftali and Au, Rhoda and Beiser, Alexa and Wolf, Philip A. and Romero, Jos{\'e} Rafael and Zhu, Yi-Cheng and Lunetta, Kathryn L. and Farrer, Lindsay and Dufouil, Carole and Kuller, Lewis H. and Mazoyer, Bernard and Seshadri, Sudha and Tzourio, Christophe and Debette, St{\'e}phanie},
  year = {2013},
  month = jul,
  journal = {Neurology},
  volume = {81},
  number = {3},
  pages = {292--300},
  publisher = {{Wolters Kluwer Health, Inc. on behalf of the American Academy of Neurology}},
  issn = {0028-3878, 1526-632X},
  doi = {10.1212/WNL.0b013e31829bfda4},
  urldate = {2023-08-24},
  abstract = {Objective: We aimed to examine the association of APOE {$\epsilon$} genotype with MRI markers of cerebrovascular disease (CVD): white matter hyperintensities, brain infarcts, and cerebral microbleeds. Methods: We performed a systematic review and meta-analysis of 42 cross-sectional or longitudinal studies identified in PubMed from 1966 to June 2012 (n = 29,965). This included unpublished data from 3 population-based studies: 3C-Dijon, Framingham Heart Study, and Sydney Memory and Ageing Study. When necessary, authors were contacted to provide effect estimates for the meta-analysis. Results: APOE {$\epsilon$}4 carrier status and APOE {$\epsilon$}44 genotype were associated with increasing white matter hyperintensity burden (sample size{\textendash}weighted z score meta-analysis [meta]-p = 0.0034 and 0.0030) and presence of cerebral microbleeds (meta odds ratio [OR] = 1.24, 95\% confidence interval [CI] [1.07, 1.43], p = 0.004, and 1.87 [1.26, 2.78], p = 0.002), especially lobar. APOE {$\epsilon$}2 carrier status was associated with increasing white matter hyperintensity load (z score meta-p = 0.00053) and risk of brain infarct (meta OR = 1.41[1.09, 1.81], p = 0.008). Conclusions: APOE {$\epsilon$}4 and APOE {$\epsilon$}2 were associated with increasing burden in MRI markers for both hemorrhagic and ischemic CVD. While the association of APOE {$\epsilon$}4 with an increased burden of CVD could be partly contributing to the relationship between APOE {$\epsilon$}4 and AD, APOE {$\epsilon$}2 was associated with MRI markers of CVD in the opposite direction compared to AD.},
  chapter = {Views \&amp; Reviews},
  copyright = {{\copyright} 2013 American Academy of Neurology},
  langid = {english},
  pmid = {23858411},
  file = {/data/hu_fbeyer/Zotero/storage/5IMEJ2UG/Schilling et al. - 2013 - APOE genotype and MRI markers of cerebrovascular d.pdf}
}

